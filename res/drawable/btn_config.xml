<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
 <item android:state_pressed="false" android:state_focused="false">
  <shape android:shape="rectangle">
   <gradient android:startColor="#CFF0" android:endColor="#C770" android:type="linear" android:angle="315" android:centerX="0.5" android:centerY="0.5" />
   <corners android:radius="4dip" />
   <stroke android:width="2dip" android:color="#CBB0" />
  </shape>
 </item>
 <item android:state_pressed="true" android:state_focused="false">
  <shape android:shape="rectangle">
   <gradient android:startColor="#C770" android:endColor="#CFF0" android:type="linear" android:angle="315" android:centerX="0.5" android:centerY="0.5" />
   <corners android:radius="4dip" />
   <stroke android:width="2dip" android:color="#CDD0" />
  </shape>
 </item>
</selector>
